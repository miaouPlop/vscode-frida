{
	"name": "vscode-frida",
	"displayName": "vscode-frida",
	"publisher": "codecolorist",
	"description": "Unofficial frida workbench for VSCode",
	"version": "0.0.1",
	"extensionKind": [
		"workspace"
	],
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:fridaApps",
		"onView:fridaPs",
		"onCommand:frida.attach",
		"onCommand:frida.spawn",
		"onCommand:frida.spawn.suspended",
		"onCommand:frida.ps.refresh",
		"onCommand:frida.app.refresh",
		"onCommand:frida.syslog",
		"onCommand:frida.syslog.vacuum",
		"onCommand:frida.browse",
		"onCommand:frida.runlast",
		"onCommand:frida.external.bagbak",
		"onCommand:frida.external.objection",
		"onCommand:frida.external.compile",
		"onCommand:frida.external.watch"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "frida.ps.refresh",
				"title": "Refresh Processes",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.apps.refresh",
				"title": "Refresh Apps",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.attach",
				"title": "Attach to",
				"icon": {
					"light": "resources/light/attach.svg",
					"dark": "resources/dark/attach.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.spawn",
				"title": "Spawn",
				"icon": {
					"light": "resources/light/statusRun.svg",
					"dark": "resources/dark/statusRun.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.spawn.suspended",
				"title": "Spawn Suspended",
				"category": "Frida"
			},
			{
				"command": "frida.kill",
				"title": "Kill Process",
				"category": "Frida"
			},
			{
				"command": "frida.browse",
				"title": "Browse Files",
				"category": "Frida"
			},
			{
				"command": "frida.syslog",
				"title": "View Logs",
				"icon": {
					"light": "resources/light/statusRun.svg",
					"dark": "resources/dark/statusRun.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.syslog.vacuum",
				"title": "Close All Console",
				"category": "Frida"
			},
			{
				"command": "frida.external.bagbak",
				"title": "Dump iOS App",
				"category": "Frida"
			},
			{
				"command": "frida.external.objection",
				"title": "Objection",
				"category": "Frida"
			},
			{
				"command": "frida.external.compile",
				"title": "Compile frida script",
				"category": "Frida"
			},
			{
				"command": "frida.external.watch",
				"title": "Watch frida script",
				"category": "Frida"
			},
			{
				"command": "frida.runlast",
				"title": "Run last frida REPL command",
				"category": "Frida"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "frida-sidebar",
					"title": "Frida Utilities",
					"icon": "resources/icon.svg"
				}
			]
		},
		"views": {
			"frida-sidebar": [
				{
					"id": "fridaApps",
					"name": "Apps"
				},
				{
					"id": "fridaPs",
					"name": "Processes"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "frida.apps.refresh",
					"when": "view == fridaApps",
					"group": "navigation@1"
				},
				{
					"command": "frida.ps.refresh",
					"when": "view == fridaPs",
					"group": "navigation@1"
				}
			],
			"view/item/context": [
				{
					"command": "frida.attach",
					"when": "view == fridaApps && viewItem == 'app|running'",
					"group": "inline"
				},
				{
					"command": "frida.attach",
					"when": "view == fridaPs && viewItem == 'process|running'",
					"group": "inline"
				},
				{
					"command": "frida.spawn",
					"when": "view == fridaApps && viewItem =~ /^app|/ && viewItem != device",
					"group": "inline"
				},
				{
					"command": "frida.spawn.suspended",
					"when": "view == fridaApps && viewItem =~ /^app|/ && viewItem != device",
					"group": "frida_process@1"
				},
				{
					"command": "frida.syslog",
					"when": "view == fridaApps && viewItem =~ /^app|/ && viewItem != device",
					"group": "frida_logger@1"
				},
				{
					"command": "frida.syslog",
					"when": "view == fridaPs && viewItem =~ /^process|/ && viewItem != device"
				},
				{
					"command": "frida.kill",
					"when": "view == fridaApps && viewItem == 'app|running'",
					"group": "frida_process@2"
				},
				{
					"command": "frida.kill",
					"when": "view == fridaPs && viewItem == 'process|running'",
					"group": "frida_process@3"
				},
				{
					"command": "frida.browse",
					"when": "view == fridaApps && viewItem =~ /^app|/ && viewItem != device",
					"group": "frida_logger@2"
				},
				{
					"command": "frida.browse",
					"when": "view == fridaPs && viewItem =~ /^process|/ && viewItem != device",
					"group": "frida_logger@3"
				},
				{
					"command": "frida.external.bagbak",
					"when": "view == fridaApps && viewItem =~ /^app|/ && viewItem != device",
					"group": "frida_external@3"
				},
				{
					"command": "frida.external.objection",
					"when": "view == fridaApps && viewItem =~ /^app|/ && viewItem != device",
					"group": "frida_external@4"
				}
			],
			"editor/context": [
				{
					"command": "frida.external.compile",
					"when": "editorLangId == typescript || resourceFilename == 'package.json'",
					"group": "frida_editor_compile@1"
				},
				{
					"command": "frida.external.watch",
					"when": "editorLangId == typescript || resourceFilename == 'package.json'",
					"group": "frida_editor_compile@2"
				}
			],
			"explorer/context": [
				{
					"command": "frida.external.compile",
					"when": "resourceLangId == typescript || resourceFilename == 'package.json'",
					"group": "frida_explorer_compile@1"
				},
				{
					"command": "frida.external.watch",
					"when": "resourceLangId == typescript || resourceFilename == 'package.json'",
					"group": "frida_explorer_compile@2"
				}
			],
			"commandPalette": [
				{
					"command": "frida.external.compile",
					"when": "resourceLangId == typescript || resourceFilename == 'package.json'",
					"group": "frida_palette_compile@1"
				},
				{
					"command": "frida.external.watch",
					"when": "resourceLangId == typescript || resourceFilename == 'package.json'",
					"group": "frida_palette_compile@2"
				},
				{
					"command": "frida.runlast",
					"group": "frida_palette_run@1"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "%ext.config.title%",
			"properties": {
				"frida.javascript.runtime": {
					"type": "string",
					"markdownDescription": "%ext.config.runtime%",
					"enum": [
						"v8",
						"duk"
					],
					"default": "v8"
				},
				"frida.device.enableRemote": {
					"type": "boolean",
					"markdownDescription": "%ext.config.remote%",
					"default": false
				},
				"frida.device.remoteAddresses": {
					"type": "array",
					"markdownDescription": "%ext.config.addr%",
					"default": []
				},
				"frida.output.log": {
					"type": "boolean",
					"markdownDescription": "%ext.config.log%",
					"default": false
				},
				"frida.output.saveScriptAndLog": {
					"type": "boolean",
					"markdownDescription": "%ext.config.save%",
					"default": false
				},
				"frida.output.saveDirectory": {
					"type": "string",
					"markdownDescription": "%ext.config.dir%",
					"default": ""
				}
			}
		},
		"keybindings": [
			{
				"command": "frida.external.compile",
				"key": "ctrl+alt+b",
				"when": "editorLangId == typescript || resourceFilename == 'package.json'"
			},
			{
				"command": "frida.external.watch",
				"key": "ctrl+alt+w",
				"when": "editorLangId == typescript || resourceFilename == 'package.json'"
			},
			{
				"command": "frida.runlast",
				"key": "ctrl+alt+r"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.31",
		"@types/vscode": "^1.41.0",
		"electron-rebuild": "^1.8.8",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"axios": "^0.19.1",
		"ws": "^7.2.1"
	}
}
